---
import Cursos from "../../data/GetDataCursos";
import Form from "./Form.jsx";
import { Icon } from "astro-icon/components";


const cursosArray = ['Seleccionar curso'];

Cursos.map(({ titulo }) => {
    cursosArray.push(titulo);
});

---


<Form client:load itemsProgramas={cursosArray}> 
    <Icon name="CheckIcon" size={50} color="#12CB54" slot="iconCheck">
</Form>


